  "capabilities": {
    "alwaysMatch": {
      "browserName": "chrome",
      "goog:loggingPrefs": {
        "browser": "ALL",
        "driver": "ALL"
      },
      "goog:chromeOptions": {
        "args": [
          "--headless=new",
          "--remote-debugging-port=9222"
        ]
      }
    }
  }
}